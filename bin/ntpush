#!/bin/bash
source ntcommon.sh

gpg -o ${ENCRYPTED_NOTES} -r jrzeszotko@gmail.com -e ${PLAINTEXT_NOTES}

cp ${ENCRYPTED_NOTES} ${MONTHLY_ENCRYPTED_NOTES}

rclone copy ${ENCRYPTED_NOTES} gdrive:Notes/
rclone copy ${MONTHLY_ENCRYPTED_NOTES} gdrive:Notes/

rm ${ENCRYPTED_NOTES}
rm ${MONTHLY_ENCRYPTED_NOTES}
